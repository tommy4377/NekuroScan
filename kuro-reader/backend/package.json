{
  "name": "kuro-auth-backend",
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "start": "tsx auth-server.ts",
    "dev": "tsx watch auth-server.ts",
    "migrate": "tsx migrate.ts",
    "repair": "tsx repair-data.ts",
    "setup": "npm run migrate && npm run repair",
    "build": "npx prisma generate",
    "migrate:deploy": "npx prisma db push",
    "migrate:dev": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.38.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "prisma": "^5.7.1",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.0",
    "@types/node": "^20.11.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "typescript": "^5.3.3"
  }
}
