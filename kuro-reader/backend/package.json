{
  "name": "kuro-auth-backend",
  "version": "2.0.1",
  "type": "module",
  "scripts": {
    "start": "node auth-server.js",
    "dev": "nodemon auth-server.js",
    "build": "npx prisma generate && npx prisma db push --accept-data-loss",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "prisma": "^5.7.1",
    "nodemon": "^3.0.2"
  }
}
